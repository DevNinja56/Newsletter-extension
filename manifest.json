{
	"manifest_version": 3,
	"name": "Newsletter",
	"version": "0.001",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js" : ["content.js"]
		}
	],
	"action": {
	    "default_popup": "popup.html",
	    "default_title": "Newsletter"
    },
    "permissions": ["management","tabs","notifications"]
  

}